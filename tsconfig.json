{
	"compilerOptions": {
		"allowJs": true,
		"watch": true,
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"declaration": true,
		"rootDir": "src",
		"skipLibCheck": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
	},
	"exclude": [
		"out",
		"node_modules",
		".vscode-test"
	]
}