{
	"name": "bebrastasksupport",
	"displayName": "BebrasTaskSupport",
	"description": "Support for Bebras task edition in Markdown",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"markdown.markdownItPlugins": true,
		"markdown.previewStyles": [
			"./bebrasmdstyle.css"
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"schemagen": "node_modules/.bin/ts-json-schema-generator -p src/json_schemas.ts -o out/schemas.json"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.1",
		"@types/glob": "^7.1.1",
		"@types/js-yaml": "^3.12.4",
		"@types/lodash": "^4.14.155",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/puppeteer": "^3.0.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-json-schema-generator": "^0.70.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@completely/cli": "0.0.10",
		"@completely/zsh-generator": "0.0.4",
		"@types/markdown-it": "^10.0.1",
		"@types/pdfjs-dist": "^2.1.4",
		"commander": "^6.2.0",
		"hasbin": "^1.2.3",
		"js-yaml": "^3.14.0",
		"jsrender": "^1.0.6",
		"katex": "^0.11.1",
		"markdown-it": "^11.0.0",
		"markdown-it-anchor": "^5.3.0",
		"markdown-it-container": "^3.0.0",
		"markdown-it-multimd-table": "^4.0.3",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"markdown-it-texmath": "^0.6.7",
		"markdown-it-toc-done-right": "^4.1.0",
		"pdf-lib": "^1.7.0",
		"pdfjs-dist": "^2.4.456",
		"puppeteer": "^3.3.0",
		"slugify": "^1.4.0"
	}
}
