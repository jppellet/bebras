{
  "name": "bebras",
  "version": "0.5.3",
  "description": "Tools to support the editing of Bebras tasks in Markdown",
  "main": "./out/main.js",
  "types": "./out/main.d.ts",
  "bin": {
    "bebras": "./out/cli/bebras.js"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "lint": "eslint src --ext .ts",
    "lint-watch": "npm-watch lint"
  },
  "watch": {
    "lint": {
      "patterns": [
        "src",
        ".eslintrc.json"
      ],
      "extensions": "ts,json"
    }
  },
  "keywords": [
    "bebras",
    "markdown",
    "yaml",
    "conversion"
  ],
  "author": {
    "name": "Jean-Philippe Pellet",
    "email": "jppellet@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/jppellet/bebras",
  "devDependencies": {
    "@types/hasbin": "^1.2.0",
    "@types/jmespath": "^0.15.0",
    "@types/js-yaml": "^4.0.2",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.14.172",
    "@types/markdown-it": "^12.0.3",
    "@types/minimatch": "^3.0.5",
    "@types/node": "=12.12",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "npm-watch": "^0.13.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "hasbin": "^1.2.3",
    "image-size": "^1.0.0",
    "jmespath": "^0.15.0",
    "js-yaml": "^4.1.0",
    "jsrender": "^1.0.11",
    "katex": "^0.16.11",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.1.2",
    "markdown-it-color": "^2.1.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-multimd-table-ext": "^4.2.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-texmath": "^0.9.1",
    "markdown-it-toc-done-right": "^4.2.0",
    "minimatch": "^3.0.4",
    "pdf-lib": "^1.16.0",
    "pdfjs-dist": "^4.4.168",
    "puppeteer": "^22.13.1",
    "slugify": "^1.6.0"
  }
}
